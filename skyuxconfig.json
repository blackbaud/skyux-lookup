{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-lookup",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "omnibar": {},
  "params": {
    "svcid": {
      "value": "skyux"
    }
  },
  "app": {
    "styles": [
      "src/app/visual-styles.scss",
      "@skyux/docs-tools/css/docs-tools.css"
    ]
  },
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "intl-tel-input",
      "@types/intl-tel-input"
    ]
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  }
}
